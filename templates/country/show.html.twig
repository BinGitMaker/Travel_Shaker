{% extends 'base.html.twig' %}

{% block title %}Country{% endblock %}

{% block body %}
    <h1>Country</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ country.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ country.name }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ country.date ? country.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Duration</th>
                <td>{{ country.duration }}</td>
            </tr>
            <tr>
                <th>Hello</th>
                <td>{{ country.hello }}</td>
            </tr>
            <tr>
                <th>Thanku</th>
                <td>{{ country.thanku }}</td>
            </tr>
            <tr>
                <th>Picture</th>
                <td>{{ country.picture }}</td>
            </tr>
            <tr>
                <th>Diving</th>
                <td>{{ country.diving }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ country.content }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ country.slug }}</td>
            </tr>
            <tr>
                <th>Links</th>
                <td>{{ country.links }}</td>
            </tr>
            <tr>
                <th>Bye</th>
                <td>{{ country.bye }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('country_index') }}">back to list</a>

    <a href="{{ path('country_edit', {'id': country.id}) }}">edit</a>

    {{ include('country/_delete_form.html.twig') }}
{% endblock %}
